   "source": [
    "Take a list of years between 1800-2030.Display all the leap year which is divisible by 5"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "0ed7dbdf",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter the starting year : 1800\n",
      "Enter the ending year : 2030\n",
      "[1820, 1840, 1860, 1880, 1920, 1940, 1960, 1980, 2000, 2020]\n"
     ]
    }
   ],
   "source": [
    "a=int(input('Enter the starting year : '))\n",
    "b=int(input('Enter the ending year : '))\n",
    "\n",
    "print([yr for yr in range(a,b+1) if (yr % 4 == 0 and yr % 5 == 0 and yr % 100 != 0) or (yr % 400 == 0)])"
   ]
